    "import cv2 as cv \n",
    "\n",
    "img = cv.imread('Resources/Photos/cat.jpg')\n",
    "\n",
    "gray = cv.cvtColor(img,cv.COLOR_BGR2GRAY)\n",
    "cv.imshow('Gray',gray)\n",
    "blur = cv.GaussianBlur(img,(3,3),cv.BORDER_DEFAULT)\n",
    "\n",
    "cv.imshow('Blur',blur)\n"
   ]
